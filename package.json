{
    "name": "webdna",
    "displayName": "WebDNA",
    "description": "WebDNA File",
    "version": "0.1.0",
    "publisher": "EduCloud",
    "repository": "https://github.com/educloud-ny/webdna-vscode",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "webdna",
            "aliases": ["WebDNA", "webdna"],
            "extensions": [".html",".htm",".tpl",".inc",".dna"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "webdna",
            "path": "./syntaxes/webdna.tmLanguage.json",
            "scopeName": "source.webdna",
            "embeddedLanguages": {
                "text.html": "html",
                "source.css": "css",
                "source.sql": "sql",
                "text.xml": "xml",
                "source.js": "javascript",
                "source.json": "json"
            }
        }]
    }
}